{
  "name": "code-challenges",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "test": "ts-node -F node_modules/.bin/tape src/**/*.spec.ts",
    "coverage": "TS_NODE_FAST=true nyc node_modules/.bin/tape src/**/*.spec.ts",
    "lint": "tslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prashaantt/code-challenges"
  },
  "author": "prashaantt@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/tape": "^4.2.28",
    "nyc": "^10.1.2",
    "tape": "^4.6.3",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "typescript": "^2.1.5"
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "cache": true
  },
  "dependencies": {}
}
